<%= turbo_frame_tag :main_table do %>
  <% if @building_presenter.able_to_show_main_table? %>
    <% if @building_presenter.recalculate_conformity == false %>
      <%= render 'buildings/table_builder/main_table/main_table' %>
    <% else %>
      <%= render 'buildings/table_builder/main_table/main_table_recalculated' %>
    <% end %>
  <% end %>
<% end %>
